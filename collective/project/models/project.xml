<?xml version="1.0" ?>
<model xmlns="http://namespaces.plone.org/supermodel/schema"
       xmlns:form="http://namespaces.plone.org/supermodel/form">
    <!-- This model is loaded at startup and used to populate the IPage
         interface in page.py.
      -->
    <schema>
      <field name="start" type="zope.schema.Datetime">
        <title>Start</title>
        <required>False</required>
      </field>
      <field name="stop" type="zope.schema.Datetime">
        <title>Stop</title>
        <required>False</required>
      </field>
      <field name="rate" type="zope.schema.Float">
        <title>Rate</title>
        <required>False</required>
      </field>
      <field name="flat" type="zope.schema.Bool">
        <default>False</default>
        <description />
        <required>False</required>
        <title>Flat</title>
      </field>
      <field name="billable" type="zope.schema.Bool">
        <default>True</default>
        <description />
        <required>False</required>
        <title>Billable</title>
      </field>
    </schema>
</model>
